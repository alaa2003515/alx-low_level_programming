
#include <stdio.h>
/**
 * a program prints largest prime factor of the number (lol)
 *@nUmBeR: num_ber to start________
 * Return: returns always (0) ______
 */

int main(void)
{
long alaa;
long int lol = 612852475143;
long lolo;
double lo = sqrt(lol);

for (alaa = 1; alaa <= lo; alaa++)
{
if (lol % alaa == 0)
{
lolo = lol / alaa;
}
}
printf("%ld\n", lolo);
return (0);
}
