#include "main.h"

/**
 *_strcat - aa function that concatenates two strings.
 *@siko: arry____
 *@miko:arry of num_ber________
 * Return: returns always (0) ______
 */

char *_strcat(char *d, char *s)
{
int siko = 0
int miko = 0;
do {
siko++;
} while (d[siko]);

while (s[miko])
{
d[siko++] = s[miko];
miko++;
}
}
